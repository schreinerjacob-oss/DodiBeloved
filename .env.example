# Notify server (optional). If set, push notifications will register and notify via this server.
# Client (Vite) - prefix with VITE_ to expose to browser:
VITE_NOTIFY_SERVER_URL=https://your-notify-server.example.com
VITE_VAPID_PUBLIC_KEY=your_base64url_vapid_public_key

# Notify server (Node) - for `npm run notify-server`:
# Generate keys: npx web-push generate-vapid-keys
VAPID_PUBLIC_KEY=
VAPID_PRIVATE_KEY=
NOTIFY_PORT=5001
